{
    "version": 2,
    "builds": [
      {
        "src": "src/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }